{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "resourceClass": "m-medium" }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote",
        "withoutCredentials": true
      },
      "ios": { "resourceClass": "m-medium" }
    },
    "ios-simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "withoutCredentials": true
      }
    },
    "production": {
      "ios": { "resourceClass": "m-medium" }
    }
  },
  "submit": {
    "production": {
      "ios": { "appleId": "your-apple-id@email.com", "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID" }
    }
  }
}
